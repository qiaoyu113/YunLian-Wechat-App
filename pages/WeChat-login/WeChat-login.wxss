.img{
    width:160rpx;
    height:160rpx;
    border-radius:100%;
    border:1rpx solid #d2d2d2;
    background:#fff;
    margin:auto;
    margin-top:30%;
    overflow: hidden;
}
.img image{
    width:150rpx;
    height:150rpx;
    border-radius: 100%;
    margin-top:5rpx;
    margin-left:5rpx;
}
.admin-name{
    width:240rpx;
    margin:auto;
    text-align:center;
    margin-top:30rpx;
}
.phone{
    width:300rpx;
    height:60rpx;
    line-height:60rpx;
    color:#fff;
    border-radius: 50rpx;
    background:#48C23D;
    margin:auto;
    text-align: center;
    font-size:26rpx;
    margin-top:30rpx;
}